#PF-Assgn-41
def find_ten_substring(num_str):
    sum = 0
    ten_list = []
    
    for i in range(0,len(num_str)) :
        print(num_str[i])
        print("first Loop")
        for j in range(i , len (num_str)) : 
            print(num_str[j])
                
            if (num_str[i] + num_str[j]) == 10 :
                print("Ten")
                print("Numbers " , num_str[i] , num_str[j]) 
                ten_list.append(num_str[i])
                ten_list.append(num_str[j])
    


num_str="2825302"
print("The number is:",num_str)
result_list=find_ten_substring(num_str)
print(result_list)