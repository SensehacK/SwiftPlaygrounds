for i in range(10) :
    
    if i == 5 :
        continue
    
    if i == 7 :
        break
    
    print(i)
    print(i+1)
    print(i**(i+1))
    
    
    

sample_tuple="A",
tuple = "C",
tty = sample_tuple + tuple
print(sample_tuple)
print(sample_tuple + tuple)
print(tty)


list1=[5,10,15,20,25]
print(len(list1))
