#PF-Assgn-41
def find_ten_substring(num_str):
    
    for i in range(0,len(num_str)) :
        print(num_str[i])
        print("first Loop")
        for j in range(i , len (num_str)) : 
            print(num_str[j])    
        
    


num_str="2825302"
print("The number is:",num_str)
result_list=find_ten_substring(num_str)
print(result_list)