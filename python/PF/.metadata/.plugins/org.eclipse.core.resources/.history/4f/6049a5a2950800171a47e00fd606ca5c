import cx_Oracle
con = cx_Oracle.Connection('t753423/t753423@10.123.79.57/georli03')
cur = cx_Oracle.Cursor(con)
try:
    cur.execute("SELECT * FROM Computer")
    con.commit()
    
except Exception as e : 
    print("Error")
    print(e)
    
except cx_Oracle.DatabaseError as e:
    print("Hello Error")
    print(e)
finally:
    print("Closing In")
    con.close()